    "@angular/compiler-cli": "^18.0.2",
    "@angular/language-service": "^18.0.2",
    "@types/core-js": "^2.5.5",
    "@types/croppie": "^2.6.1",
    "@types/express": "^4.17.17",
    "@types/jasmine": "^4.3.2",
