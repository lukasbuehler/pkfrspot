{
  "name": "pkfrspot",
  "version": "1.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "serve": "ng serve",
    "dev": "ng serve --watch",
    "build": "ng build && node copy-proxy-server.js",
    "build:dev": "ng build --configuration=development && node copy-proxy-server.js",
    "build:prod": "ng build --configuration=production && node copy-proxy-server.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve:ssr": "node dist/pkfrspot/server/server.mjs",
    "serve:ssr:pkfrspot": "node dist/pkfrspot/server/server.mjs",
    "deploy:test:rules": "firebase use test && firebase deploy --only firestore:rules",
    "deploy:prod:rules": "firebase use prod && firebase deploy --only firestore:rules",
    "deploy:test:functions": "firebase use test && cd ../functions && npm run build && firebase deploy --only functions",
    "deploy:prod:functions": "firebase use prod && cd ../functions && npm run build && firebase deploy --only functions",
    "tauri": "tauri"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.1.1",
    "@angular/cdk": "^18.1.1",
    "@angular/common": "^18.1.1",
    "@angular/compiler": "^18.1.1",
    "@angular/core": "^18.1.1",
    "@angular/fire": "^18.0.1",
    "@angular/forms": "^18.1.1",
    "@angular/google-maps": "^18.1.1",
    "@angular/localize": "^18.1.1",
    "@angular/material": "^18.1.1",
    "@angular/platform-browser": "^18.1.1",
    "@angular/platform-browser-dynamic": "^18.1.1",
    "@angular/platform-server": "^18.1.1",
    "@angular/router": "^18.1.1",
    "@angular/service-worker": "^18.1.1",
    "@angular/ssr": "^18.2.1",
    "@firebase/firestore": "^4.6.4",
    "@videogular/ngx-videogular": "^8.0.0",
    "bootstrap": "^5.3.0",
    "croppie": "^2.6.5",
    "express": "^4.18.2",
    "firebase": "^10.12.4",
    "i18n-iso-countries": "^7.11.3",
    "rxjs": "^7.8.1",
    "swiper": "^11.1.7",
    "timers": "^0.1.1",
    "typesense": "^1.7.2",
    "uuid": "^10.0.0",
    "xml2js": "^0.6.0",
    "zone.js": "^0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.1",
    "@angular-devkit/core": "^18.2.1",
    "@angular/cli": "^18.2.1",
    "@angular/compiler-cli": "^18.1.1",
    "@angular/language-service": "^18.1.1",
    "@tauri-apps/cli": "^2.1.0",
    "@types/core-js": "^2.5.5",
    "@types/croppie": "^2.6.1",
    "@types/express": "^4.17.17",
    "@types/node": "^18.18.0",
    "@types/xml2js": "^0.4.9",
    "ng-extract-i18n-merge": "^2.12.0",
    "sass": "^1.63.3",
    "typescript": "5.4"
  },
  "overrides": {
    "undici": "6.19.2",
    "undici-types": "6.19.2"
  }
}
