  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration=development",
    "dev": "ng serve --configuration=development --watch",
    "build:test": "ng build --configuration=test",
    "build:prod": "ng build --configuration=production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy:test:hosting": "firebase use test && ng build --configuration=test && firebase deploy --only hosting",
    "deploy:prod:hosting": "firebase use prod && ng build --configuration=production && firebase deploy --only hosting",
    "deploy:test:rules": "firebase use test && firebase deploy --only firestore:rules",
    "deploy:prod:rules": "firebase use prod && firebase deploy --only firestore:rules"
  },
